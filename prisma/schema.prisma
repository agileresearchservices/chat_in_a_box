generator client {
  provider = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  extensions = [vector]
}

model docs {
  id        Int      @id @default(autoincrement())
  doc_id    String
  source    String
  type      String
  chunk     String
  embedding Unsupported("vector")?
  createdAt DateTime @default(now())
}